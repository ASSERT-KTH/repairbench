             position.set(Position.PREFIX_ADC + 2, parser.nextDouble() * 0.001);
         }
 
         Integer lac = parser.nextHexInt();
         Integer cid = parser.nextHexInt();
         Integer mcc = parser.nextInt();
         Integer mnc = parser.nextInt();
