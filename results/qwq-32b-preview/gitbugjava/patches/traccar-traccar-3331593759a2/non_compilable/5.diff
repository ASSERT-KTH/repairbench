+private void validateAndAdjustCoordinates(Position position) {
+    double latitude = position.getLatitude();
+    double longitude = position.getLongitude();
+    if (latitude < -90 || latitude > 90 || longitude < -180 || longitude > 180) {
+        if (latitude < -90 || latitude > 90) {
+            log.warn("Invalid latitude value: {}. Adjusting to nearest valid value.", latitude);
+            position.setLatitude(Math.max(-90, Math.min(90, latitude)));
+        }
+        if (longitude < -180 || longitude > 180) {
+            log.warn("Invalid longitude value: {}. Adjusting to nearest valid value.", longitude);
+            position.setLongitude(Math.max(-180, Math.min(180, longitude)));
+        }
+    }
