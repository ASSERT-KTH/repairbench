+    }
+    if (BitUtil.check(status, 2)) {
+        position.set(Position.PREFIX_IO + 3, 1);
+    }
+    if (BitUtil.check(status, 3)) {
+        position.set(Position.PREFIX_IO + 4, 1);
     }
 
