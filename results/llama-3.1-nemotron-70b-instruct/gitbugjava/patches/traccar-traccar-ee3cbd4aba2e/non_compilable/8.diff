+                    short[] gyro = new short[3];
+                    for (int i = 0; i < 3; i++) {
+                        gyro[i] = buf.readShort();
+                    }
+                    position.set("gyro", gyro);
+                } else {
+                    buf.skipBytes(length);
+                }
                 break;
