+// Added helper method to decode coordinates correctly
+private void decodeCoordinates(Position position, ByteBuf buf) {
+    // Read raw latitude and longitude as integers
+    int latitude = buf.readInt();
+    int longitude = buf.readInt();
+
+    // Convert to float and scale by 1e5
+    float lat = latitude / 1.0f / (float) (1 << 20);
+    float lon = longitude / 1.0f / (float) (1 << 20);
+
+    // Ensure values are within valid ranges
+    if (lat < -90.0f) lat = -90.0f;
+    else if (lat > 90.0f) lat = 90.0f;
+    if (lon < -180.0f) lon = -180.0f;
+    else if (lon > 180.0f) lon = 180.0f;
+
+    position.setLatitude(lat);
+    position.setLongitude(lon);
+}
