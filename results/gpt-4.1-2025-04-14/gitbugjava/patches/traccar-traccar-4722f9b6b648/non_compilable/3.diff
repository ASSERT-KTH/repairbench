+
+    buf.readUnsignedIntLE(); // date and time (ignore as Iridium time not reliable/provided)
 
     buf.skipBytes(23); // coordinates block
 
     buf.skipBytes(3); // data tag header
     decodeMinimalDataSet(position, buf);
 
     return position;
 }
