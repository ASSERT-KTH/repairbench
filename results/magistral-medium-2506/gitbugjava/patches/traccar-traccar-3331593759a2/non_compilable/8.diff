+            if (lat > 90 || lat < -90) {
+                // Handle invalid latitude - you might want to log or throw an exception
+                lat = 0;
+            }
+            // Validate longitude before setting it
+            if (lon > 180 || lon < -180) {
+                // Handle invalid longitude
+                lon = 0;
+            }
+
+            position.setLatitude(lat);
+            position.setLongitude(lon);
+            position.setAltitude(alt);
